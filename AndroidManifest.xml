<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="cm.aptoide.pt"
      android:versionName="1.0.4" android:versionCode="51">
      <uses-permission android:name="android.permission.INTERNET" />
      <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application android:label="@string/app_name" android:icon="@drawable/icon">
        <activity android:name=".Aptoide"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
            	<action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="http" />
                <data android:scheme="https" />
                <data android:scheme="file" />
                <data android:host="*" />  
                <data android:mimeType="application/vnd.cm.aptoide.pt" />
            </intent-filter>
            
            
        </activity>
    
<activity android:name="SdIn"></activity>



<activity android:name="ManageRepo"></activity>

<activity android:name="Settings"></activity>
<activity android:name="RemoteInSearch"
			android:launchMode="singleTop">
	<intent-filter>
    	<action android:name="android.intent.action.SEARCH" />
		<category android:name="android.intent.category.DEFAULT" />
	</intent-filter>
	<meta-data android:name="android.app.searchable" android:resource="@xml/find"/>
</activity>
<activity android:name="RemoteInTab">
	            <meta-data android:name="android.app.default_searchable"
                       android:value=".RemoteInSearch" />
</activity>
</application>
    <uses-sdk android:minSdkVersion="2" />
</manifest> 